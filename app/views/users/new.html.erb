<% provide(:title, 'Sign Up') %>

<% if !logged_in? %>
  <div class="row">
    <div class="col-md-offset-4 col-md-4 col-xs-12">
      <%= render 'signup' %>
    </div>
  </div>
<% else %>
  <p>Please <%= link_to "Log out", logout_path, method: "delete" %> if you want to sign up as a new user.</p>
<% end %>

