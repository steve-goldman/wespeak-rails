<% provide(:title, "My Groups") %>
<div class="row">
  <div class="col-md-offset-3 col-md-6 col-xs-12">
    <h4>My Groups (<%= current_user.active_members.count %>)</h4>
    <ul id="groups-list" class="list-group groups-list">
      <%= render 'group_pages/group_rows', groups: @my_groups %>
    </ul>
    <div id="infinite-scrolling">
      <%= will_paginate @my_groups %>
    </div>
  </div>
</div>
